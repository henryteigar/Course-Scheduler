{
  "name": "course-scheduler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack.config.js; node server/server.js",
    "dev": "concurrently --kill-others \"webpack-dev-server --config webpack.config.js --content-base client\" \"nodemon server/server.js\"",
    "prod": "webpack --config webpack.config.js; npm run pm2-start",
    "pm2-stop": "pm2 stop server/server.js",
    "pm2-start": "pm2 stop server/server.js; pm2 start server/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "pm2": "^2.7.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "babel-core": "^6.26.0",
    "babel-loader": "6.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
